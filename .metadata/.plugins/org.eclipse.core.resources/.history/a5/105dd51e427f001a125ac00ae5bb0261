public class Main {
	
	public static void mapYazdir(Map<Integer, String> map) {
		map.put(5, "Java");
		map.put(2, "Javascript");
		map.put(15, "php");
		map.put(35, "c");
		map.put(100, "css");
		
		
//		for(Map.Entry<Integer, String> entry : map.entrySet()) {
