package StacklerleBeraberKullanimi;
//ExceptionPropagations
public class Main {
	
	public static void ununcuFonksiyon() {
		try {
		int a = 12/0;
		}
		c
		}
	public static void ikinciFonksiyon() {
		ununcuFonksiyon();
	}
	public static void birinciFonksiyon() {
		ikinciFonksiyon();
	}
	
public static void main(String args[]) {
	birinciFonksiyon();
}
}
